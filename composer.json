{
    "name": "mmorinaga/cakephp3-aclmanager",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cake3", "plugin", "acl"],
    "homepage": "https://github.com/mmorinaga/CakePhp3-AclManager",
    "description": "An acl-manager plugin for CakePhp3",
    "authors": [
        {
            "name": "masato morinaga",
            "email": "masato.morinaga@gmail.com",
            "homepage": "https://github.com/mmorinaga"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.4",
        "cakephp/acl": "dev-master"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AclManager\\": "src"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}
